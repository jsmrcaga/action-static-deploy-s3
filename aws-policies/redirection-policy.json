{
    "RoutingRules": [
        {
            "Redirect": {
                "ReplaceKeyWith": "index.html"
            }
        },
        {
            "Condition": {
                "KeyPrefixEquals": "static/"
            },
            "Redirect": {
                "ReplaceKeyPrefixWith": "static/"
            }
        },
        {
            "Condition": {
                "HttpErrorCodeReturnedEquals": "403"
            },
            "Redirect": {
                "ReplaceKeyWith": "index.html"
            }
        }
    ]
}
